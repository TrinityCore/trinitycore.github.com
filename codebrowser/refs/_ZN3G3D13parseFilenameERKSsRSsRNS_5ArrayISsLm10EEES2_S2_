<dec f='TrinityCore3.3.5/dep/g3dlite/include/G3D/fileutils.h' l='94' type='void G3D::parseFilename(const std::string &amp; filename, std::string &amp; drive, Array&lt;std::string&gt; &amp; path, std::string &amp; base, std::string &amp; ext)'/>
<doc f='TrinityCore3.3.5/dep/g3dlite/include/G3D/fileutils.h' l='70'>/**
  Parses a filename into four useful pieces.

  Examples:

  c:\\a\\b\\d.e   
    root  = &quot;c:\\&quot;
    path  = &quot;a&quot; &quot;b&quot;
    base  = &quot;d&quot;
    ext   = &quot;e&quot;
 
  /a/b/d.e
    root = &quot;/&quot;
    path  = &quot;a&quot; &quot;b&quot;
    base  = &quot;d&quot;
    ext   = &quot;e&quot;

  /a/b
    root  = &quot;/&quot;
    path  = &quot;a&quot;
    base  = &quot;b&quot;
    ext   = &quot;e&quot;

 */</doc>
