<dec f='TrinityCore3.3.5/dep/recastnavigation/Recast/Include/Recast.h' l='765' type='void rcMarkWalkableTriangles(rcContext * ctx, const float walkableSlopeAngle, const float * verts, int nv, const int * tris, int nt, unsigned char * areas)'/>
<def f='TrinityCore3.3.5/dep/recastnavigation/Recast/Source/Recast.cpp' l='247' type='void rcMarkWalkableTriangles(rcContext * ctx, const float walkableSlopeAngle, const float * verts, int , const int * tris, int nt, unsigned char * areas)'/>
<doc f='TrinityCore3.3.5/dep/recastnavigation/Recast/Source/Recast.cpp' l='239'>/// @par
///
/// Only sets the area id&apos;s for the walkable triangles.  Does not alter the
/// area id&apos;s for unwalkable triangles.
/// 
/// See the #rcConfig documentation for more information on the configuration parameters.
/// 
/// @see rcHeightfield, rcClearUnwalkableTriangles, rcRasterizeTriangles</doc>
<doc f='TrinityCore3.3.5/dep/recastnavigation/Recast/Include/Recast.h' l='754'>/// Sets the area id of all triangles with a slope below the specified value
/// to #RC_WALKABLE_AREA.
///  @ingroup recast
///  @param[in,out]	ctx					The build context to use during the operation.
///  @param[in]		walkableSlopeAngle	The maximum slope that is considered walkable.
///  									[Limits: 0 &lt;= value &lt; 90] [Units: Degrees]
///  @param[in]		verts				The vertices. [(x, y, z) * @p nv]
///  @param[in]		nv					The number of vertices.
///  @param[in]		tris				The triangle vertex indices. [(vertA, vertB, vertC) * @p nt]
///  @param[in]		nt					The number of triangles.
///  @param[out]	areas				The triangle area ids. [Length: &gt;= @p nt]</doc>
<dec f='TrinityCore6.x/dep/recastnavigation/Recast/Include/Recast.h' l='765' type='void rcMarkWalkableTriangles(rcContext * ctx, const float walkableSlopeAngle, const float * verts, int nv, const int * tris, int nt, unsigned char * areas)'/>
