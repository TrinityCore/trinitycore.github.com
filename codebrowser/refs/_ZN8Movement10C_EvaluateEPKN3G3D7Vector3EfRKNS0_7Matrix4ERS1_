<doc f='TrinityCore3.3.5/src/server/game/Movement/Spline/Spline.cpp' l='73'>/*  classic view:
inline void C_Evaluate(const Vector3 *vertice, float t, const float (&amp;matrix)[4][4], Vector3 &amp;position)
{
    Vector3 tvec(t*t*t, t*t, t);
    int i = 0;
    double c;
    double x = 0, y = 0, z = 0;
    while ( i &lt; 4 )
    {
        c = matrix[0][i]*tvec.x + matrix[1][i]*tvec.y + matrix[2][i]*tvec.z + matrix[3][i];

        x += c * vertice-&gt;x;
        y += c * vertice-&gt;y;
        z += c * vertice-&gt;z;

        ++i;
        ++vertice;
    }

    position.x = x;
    position.y = y;
    position.z = z;
}*/</doc>
