<doc f='TrinityCore3.3.5/dep/g3dlite/include/G3D/FileSystem.h' l='565'>/**
      Parses a filename into four useful pieces.

      Examples:

      c:\\a\\b\\d.e   
        root  = &quot;c:\\&quot;
        path  = &quot;a&quot; &quot;b&quot;
        base  = &quot;d&quot;
        ext   = &quot;e&quot;
     
      /a/b/d.e
        root = &quot;/&quot;
        path  = &quot;a&quot; &quot;b&quot;
        base  = &quot;d&quot;
        ext   = &quot;e&quot;

      /a/b
        root  = &quot;/&quot;
        path  = &quot;a&quot;
        base  = &quot;b&quot;
        ext   = &quot;e&quot;

     */</doc>
